{
  "name": "ConstructPro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npm run android:prebuild && react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "bundle-android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build-debug-offline": "npm run bundle-android && cd android && gradlew.bat assembleDebug",
    "android:clean": "cd android && gradlew.bat clean && cd ..",
    "android:prebuild": "cd android && pre-build-cleanup.bat && cd ..",
    "android:rebuild": "npm run android:prebuild && npm run android:clean && npm run android",
    "generate-icons": "node generate-icons-from-logo.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.7",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-ml-kit/face-detection": "^2.0.1",
    "@react-navigation/bottom-tabs": "^7.5.0",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.5.1",
    "@react-navigation/stack": "^7.5.0",
    "axios": "^1.12.2",
    "expo-blur": "^15.0.7",
    "expo-camera": "^17.0.8",
    "expo-linear-gradient": "^15.0.7",
    "react": "19.0.0",
    "react-native": "0.78.0",
    "react-native-fast-tflite": "^1.6.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.29.0",
    "react-native-image-resizer": "^1.4.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-ml-kit": "^0.0.1",
    "react-native-reanimated": "^4.1.3",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.18.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-vision-camera": "^4.7.2",
    "react-native-webview": "^13.16.0",
    "react-native-worklets": "^0.6.1",
    "react-native-worklets-core": "^1.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.0",
    "@react-native/eslint-config": "0.78.0",
    "@react-native/metro-config": "0.78.0",
    "@react-native/typescript-config": "0.78.0",
    "@types/react": "^19.0.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "sharp": "^0.34.5",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
